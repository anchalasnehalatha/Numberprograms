import java.util.Scanner;
class Example{
    public static void main(String[] args){
Scanner sc=new Scaner(System.in);
        int year = 2020;

        if ((year % 400 == 0) || ((year % 4 == 0) && (year % 100 != 0))) {
            System.out.println(year + " is leap year");
        } else {
            System.out.println(year + " is not leap year");
        }
    }
}

